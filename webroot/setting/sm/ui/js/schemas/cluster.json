                    "admin_user": {
                      "type": "string",
                      "default": "admin"
                    },
                    "admin_tenant": {
                      "type": "string",
                      "default": "admin"
                    },
                    "service_tenant": {
                      "type": "string",
                      "default": "services"
                    },
                    "auth_port": {
                      "type": "integer",
                      "default": 35357
                    },
                    "auth_protocol": {
                      "type": "string",
                      "default": "http"
