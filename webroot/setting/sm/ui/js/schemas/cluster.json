                    "external_vip": {
                      "type": "string",
                      "pattern": "^$|^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(/[0-9][0-9])?$",
                      "default": ""
                    },
                    "internal_virtual_router_id": {
                      "type": "integer",
                      "default": 102
                    },
                    "external_virtual_router_id": {
                      "type": "integer",
                      "default": 101
                    },
                    "nfs_server": {
                      "type": "string",
                      "pattern": "^$|^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(/[0-9][0-9])?$",
                      "default": ""
