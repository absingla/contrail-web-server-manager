                "openstack_manage_amqp": {
                  "type": "boolean",
                  "default": false
                },
                "enable_ceilometer": {
                  "type": "boolean",
                  "default": false
                },
                "ha": {
                  "type": "object",
                  "properties": {
                    "internal_vip": {
                      "type": "string",
                      "pattern": "^$|^(([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])\\.){3}([0-9]|[1-9][0-9]|1[0-9]{2}|2[0-4][0-9]|25[0-5])(/[0-9][0-9])?$",
                      "default": ""
